{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.{png,jpg,jpeg,svg,gif,ico,txt,xml,webp,woff,woff2}"
    ]
  },
  "mimeTypes": {
    ".js": "text/javascript; charset=utf-8",
    ".mjs": "text/javascript; charset=utf-8",
    ".css": "text/css; charset=utf-8",
    ".map": "application/json; charset=utf-8",
    ".svg": "image/svg+xml"
  }
}
